{
  "name": "onebtn-upgrade",
  "version": "1.0.0",
  "description": "upgrade multiple devices",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aviosys101/onebtnupgrade.git"
  },
  "keywords": [],
  "author": "GitHub",
  "license": "ISC",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-deb": "^6.0.0-beta.57",
    "@electron-forge/maker-dmg": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.57",
    "electron": "^12.0.10",
    "electron-wix-msi": "^3.2.0"
  },
  "dependencies": {
    "@electron-forge/maker-wix": "^6.0.0-beta.57",
    "dgram": "^1.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "form-data": "^4.0.0",
    "ip": "^1.1.5"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "asar": true,
        "icon": "css\\app.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-wix",
          "config": {
            "ui": {
              "chooseDirectory": true
            }
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "name": "electron-onebtn-upgrade"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "name": "electron-onebtn-upgrade"
          }
        }
      ]
    }
  },
  "bugs": {
    "url": "https://github.com/aviosys101/onebtnupgrade/issues"
  },
  "homepage": "https://github.com/aviosys101/onebtnupgrade#readme"
}
